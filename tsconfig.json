{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@redux/*": ["src/redux/*"],
      "@components/*": ["src/components/*"],
      "@types/*": ["src/types/*"]
    }
  },
  "include": ["src"]
}
